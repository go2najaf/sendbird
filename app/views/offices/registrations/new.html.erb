<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

   <div class="field">
    <%= f.label :business_category %><br />
    <%= f.text_field :business_category, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :business_name %><br />
    <%= f.text_field :business_name, autofocus: true %>
  </div>

   <div class="field">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name, autofocus: true %>
  </div>

    <div class="field">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :dob %><br />
    <%= f.text_field :dob, autofocus: true %>
  </div>


  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" %>
  </div>

     <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address, autofocus: true %>
  </div>

     <div class="field">
    <%= f.label :apt_num %><br />
    <%= f.text_field :apt_num, autofocus: true %>
  </div>

     <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city, autofocus: true %>
  </div>

     <div class="field">
    <%= f.label :state %><br />
    <%= f.text_field :state, autofocus: true %>
  </div>

     <div class="field">
    <%= f.label :zip_code %><br />
    <%= f.text_field :zip_code, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :card_name %><br />
    <%= f.text_field :card_name, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :card_number %><br />
    <%= f.text_field :card_number, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :exp_month %><br />
    <%= f.text_field :exp_month, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :exp_year %><br />
    <%= f.text_field :exp_year, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :cvv %><br />
    <%= f.text_field :cvv, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :billing_address %><br />
    <%= f.text_field :billing_address, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :billing_apt_num %><br />
    <%= f.text_field :billing_apt_num, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :billing_state %><br />
    <%= f.text_field :billing_state, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :billing_city %><br />
    <%= f.text_field :billing_city, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :billing_zip_code %><br />
    <%= f.text_field :billing_zip_code, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :routing_number %><br />
    <%= f.text_field :routing_number, autofocus: true %>
  </div>
   <div class="field">
    <%= f.label :account_number %><br />
    <%= f.text_field :account_number, autofocus: true %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>

